[Unit]
Description={{ artifactid }}
After=syslog.target

[Service]
WorkingDirectory={{ springboot_deploy_folder }}/
User={{ springboot_user }}
ExecStart={{ java_dir }}/bin/java -jar -Dspring.profiles.active={{ springboot_profile }} -Dspring.cloud.config.label={{ config_version }} {{ springboot_deploy_folder }}/{{ artifactid }}-{{ version }}.jar
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target